body
    font-family sans-serif

aside
    float right

#main
    max-width 1000px
    margin auto

.well
    box-shadow none
    border none
    background #f8f8f8

#map
    .tile
        width 50px
        height 50px
        box-shadow inset 1px 1px 3px rgba(0, 0, 0, 0.2)
        display block
        float left

    .row
        clear left